            type: string
            default: "PLICalculateRate"
          EventVersion:
            title: "Event Version"
            type: string
            default: "1.2.0.1"
          ApplicableFromDate:
            title: "Applicable From Date"
            type: string
            default: "2017-07-01"
          NumOfProviders:
            title: "Number of Providers"
            type: string
          SharedOrSeparateLimit:
            title: "Shared or Separate Limit"
            type: string
