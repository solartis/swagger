swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis Rate API for Coverwallet
  description: To get rate for Profesional Liability from Solartis Rate API with ISO ERC.
  
  
host: cw.alpha.solartis.net
basePath: /DroolsV6/DroolsService
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Get Rates for the given input
      operationId: getRates
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "PLICalculateRate"
        - name: EventVersion
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "1.2.0.1"
        - name: Rating
          in: body
          description: Rating content
          required: true
          schema:
            $ref: '#/definitions/Rating_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Rating_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

definitions:
  Rating_Request:
    type: object
    required:
      - ServiceRequestDetail
      - Policy
    properties:
      EndClientUserUniqueSessionId:
        type: string
        default: "Uniquesession"
      EndClientUserBrowserSessionId:
        type: string
      EndClientUserBrowserInformation:
        type: string
      PaaSClientIPAddress:
        type: string
      ClientUniqueRequestID:
        type: string
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "27"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          UserName:
            type: string
            default: "CWAgent"
          Token:
            type: string
            default: ""
          BrowserIp:
            type: string 
            default: "127.0.0.1"
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
      Policy:
        type: object
        required: 
          - PolicyType
          - ZipCode
          - State
          - EventName
          - EventVersion
          - NumOfProviders
          - SharedOrSeparateLimit
          - PLOrWLCoverageType
          - LiabilityLimitsOfInsurance
          - WorkplaceLiabilityInsurance
          - CorporationOrPartnershipCoverage
          - TransitionFactor
          - EmploymentCategory
          - EffectiveDate
        properties:
          PolicyType:
            title: "Policy Type"
            type: string
            enum: ["Individual","Group"]
          ZipCode:
            title: "Zip Code"
            type: string
          State:
            title: "State"
            type: string
            enum: ["AL","AZ","CA","DC","DE","GA","IA","KY","MD","ME","MI","MN","NC","OH","PA","SC","TN","TX","UT","VA"]
          EffectiveDate:
