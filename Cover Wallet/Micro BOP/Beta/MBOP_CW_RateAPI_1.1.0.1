swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis Rate API for Coverwallet
  description: To get rate for Micro BOP from Solartis Rate API with ISO ERC.
  
  
host: cwmicrobop.beta.solartis.net
basePath: /KnowledgeEngineV6_2/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Get Rates for the given input
      operationId: getRates
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "MicroBOPCalculateRate"
        - name: EventVersion
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "1.1.0.1"
        - name: Rating
          in: body
          description: Rating content
          required: true
          schema:
            $ref: '#/definitions/Rating_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Rating_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

definitions:
  Rating_Request:
    type: object
    required:
      - Policy
    properties:
      Policy:
        type: object
        required: 
          - State
          - Location
        properties:
          EventName:
            title: "Event Name"
            type: string
            default: "MicroBOPCalculateRate"
          EventVersion:
            title: "Event Version"
            type: string
            default: "1.1.0.1"
          LastRateLookUpDate:
            title: "Last Rate LookUp Date"
            type: string
            default: ""
          PolicyEffectiveDate:
           title: "Policy Effective Date"
           type: string
          State:
            title: "State"
            type: string
            default: ""
          ProductNumber:
            title: "Product Number"
            type: string
            default: "HO_BOP"
          ProductVerNumber:
            title: "Product Version Number"
            type: string
            default: "1.0"
          Location:
            type: array
            items:
              properties:
                LocationDetail:
                  type: object
                  properties:
                    LocationId:
                      title: "Location Id"
                      type: string
                    Building:
                      type: array
                      items:
                        properties:
                          BuildingDetail:
                            type: object
                            properties:
                              BuildingId:
                                title: "Building Id"
                                type: string
                              Classification:
                                type: array
                                items:
                                  properties:
                                    ClassificationDetail:
                                      type: object
                                      properties:
                                        ClassificationId:
                                          title: "ClassificationId"         
                                          type: string
                                        Description:
                                          title: "Description"
                                          type: string
                                          enum: ["Architectural Services Office","Engineering Services Office","Consultants - Decorating Services, Interior","Consultants-Advertising","Consultants-Audio/Visual","Consultants-Business/Management","Consultants-Communications, Media and Public Relations","Consultants-Compensation & Benefits","Consultants-Educational","Consultants-Event Planning","Consultants-Executive Coaching","Consultants-Graphic Design","Consultants-Human Resources","Consultants-Image","Consultants-Market Research","Consultants-Procurement","Consultants-Project Management","Consultants-Recruiting","Consultants-Sales & Marketing","Consultants-Strategic Planning","Consultants-Training & Development","Accounting & Bookkeeping Offices","Answering Service","Auditors Offices","Auto Appraisers Offices","Certified Public Accountants Office","Claim Adjuster - Independent","Court Reporter","Graphic Design Services Office","Inspection and Appraisal Companies","Insurance Adjusters Offices","Interpreter or Translator","Learning Center - Other Educational Programs","Learning Center - Test Prep and Test Administration","Learning Center - Tutoring Services","Market Research Firm","Mortgage Brokers Offices","Notaries Office","Real Estate Agents & Brokers Office","Stenographic & Secretarial Services","Surveying/Mapping Service Office","Tax Preparation Services","Training - Professional & Mgmt Development","Travel Agencies","Pet Grooming","Pet Grooming Services - Mobile","Pet Training","Photo Studios - Portrait","Photography (Commercial)","Veterinary Services Domestic Only","Videographers","Computer Consultants","Computer Related Services - Low Hazard","Computer Related Services - Medium Hazard","Computer Systems Integrators","Computer Training","Custom Computer Programming","Packaged Software Development","Software Research And Development - No Products","Website Design"]
                                        ClassGroup:
                                          title: "Class Group"
                                          type: string
                                          enum: ["Architects, Engineers & Surveyors","Consultants","Office Professional","Service","Technology"]
                                        ExposureBase:
                                          title: "Exposure Base"
                                          type: string
                                          enum: ["Area","Payroll","Sales"]
                                        Exposure:
                                          title: "Exposure"
                                          type: string
                                        NAIC:
                                          title: "NAIC"
                                          type: string
      OwnerId:
        type: string
        default: "27"
      ServiceRequestDetail:
        type: object
        required:
          - Token
        properties:
          ServiceRequestVersion:
            title: "Service Request Version"
            type: string
            default: "1.0"
          UserName:
            title: "User Name"
            type: string
            default: "CWAgent"
          OwnerId:
            title: "Owner Id"
            type: string
            default: "27"
          RegionCode:
            title: "Region Code"
            type: string
            default: "US"
          Token:
            title: "Token"
            type: string
            default: ""
          BrowserIp:
            title: "Browser Ip"
            type: string
            default: "127.0.0.1"
          ResponseType:
            title: "Response Type"
            type: string
            default: "JSON"
      EndClientUserUniqueSessionId:
        type: string
        default: "Uniquesession"

  Rating_Success_Response:
    type: object
    required:
      - Policy
    properties:
      Policy:
        type: object
        properties:
          EventName:
            title: "Event Name"
            type: string
          EventVersion:
            title: "Event Version"
            type: string
          BasePremium:
            type: string
          Premium:
            type: string
          ProductNumber:
            title: "Product Number"
            type: string
          State:
            title: "State"
            type: string
          ProductVerNumber:
            title: "Product Version Number"
            type: string
          PolicyEffectiveDate:
            title: "Policy Effective Date"
            type: string
          LastRateLookUpDate:
            title: "Last Rate LookUp Date"
            type: string
          TotalTFSAmount:
            type: string
          Location:
            type: array
            items:
              properties:
                LocationDetail:
                  type: object
                  properties:
                    LocationId:
                      title: "Location Id"
                      type: string
                    Building:
                      type: array
                      items:
                        properties:
                          BuildingDetail:
                            type: object
                            properties:
                              BuildingId:
                                title: "Building Id"
                                type: string
                              Classification:
                                type: array
                                items:
                                  properties:
                                    ClassificationDetail:
                                      type: object
                                      properties:
                                        ClassificationId:
                                          title: "ClassificationId"         
                                          type: string
                                        Description:
                                          title: "Description"
                                          type: string
                                        ClassGroup:
                                          title: "Class Group"
                                          type: string
                                        ExposureBase:
                                          title: "Exposure Base"
                                          type: string
                                        Exposure:
                                          title: "Exposure"
                                          type: string
                                        NAIC:
                                          title: "NAIC"
                                          type: string

      OwnerId:
        type: string
      ServiceRequestDetail:
        type: object
        required:
          - Token
        properties:
          ServiceRequestVersion:
            title: "Service Request Version"
            type: string
            default: "1.0"
          UserName:
            title: "User Name"
            type: string
            default: "CWAgent"
          OwnerId:
            title: "Owner Id"
            type: string
            default: "27"
          RegionCode:
            title: "Region Code"
            type: string
            default: "US"
          Token:
            title: "Token"
            type: string
            default: ""
          BrowserIp:
            title: "Browser Ip"
            type: string
            default: "127.0.0.1"
          ResponseType:
            title: "Response Type"
            type: string
            default: "JSON"
            
      EndClientUserUniqueSessionId:
        type: string
        default: "Uniquesession"       
      
       
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string      

 
