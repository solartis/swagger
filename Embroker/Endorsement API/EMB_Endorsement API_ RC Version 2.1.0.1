swagger: '2.0'
info:
  version: "1.0"
  title: Solartis Create Endorsement API for Embroker
  description: To create endorsement for Embroker
  
host: embroker.rc.solartis.net
basePath: /KnowledgeEngineV6_2/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
