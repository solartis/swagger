swagger: '2.0'
info:
  version: "1.0"
  title: Solartis Get Policy for Embroker
  description: To enable Get Policy API for Embroker
  
host: embroker.alpha.solartis.net
basePath: /KnowledgeEngineV6_2/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Get Policy in Solartis platform
      operationId: getpolicy
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName  
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "EmbrokerGetPolicy"
        - name: EventVersion
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "1.0.0.1"
        - name: Policy
          in: body
          description: Policy content
          required: true
          schema:
            $ref: '#/definitions/Policy_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Policy_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
            
definitions:
  Policy_Request:
    type: object
    required:
      - ServiceRequestDetail
      - Policy
    properties:
      EndClientUserUniqueSessionId:
        type: string
        default: "Uniquesession"
      EndClientUserBrowserSessionId:
        type: string
      EndClientUserBrowserInformation:
        type: string
      PaaSClientIPAddress:
        type: string
      ClientUniqueRequestID:
        type: string
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "36"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          UserName:
            type: string
            default: "EmbrokerAgent"
          Token:
            type: string
            default: ""
          BrowserIp:
            type: string 
            default: "127.0.0.1"
          LanguageCode:
            type: string
            default: "en"
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
      Policy:
        type: object
        required:
          - ProductNumber
          - ProductVerNumber
          - EventName
          - EventVersion
          - PolicyNumber
        properties:
          ProductNumber:
            title: "Product Number"
            type: string
            default: "EMBRKR"
          ProductVerNumber:
            title: "Product Ver Number"
            type: string
            default: "EMBRKR_2018_V1"
          EventName:
            title: "Event Name"
            type: string
            default: "EmbrokerSubmission"
          EventVersion:
            title: "Event Version"
            type: string
            default: "1.0.0.1"
          PolicyNumber:
            title: "Policy Number"
            type: string
           
      OwnerId:
        title: "Owner ID"
        type: string
        default: "36"
        
  Policy_Success_Response:
    type: object
    required:
      - ServiceRequestDetail
      - Policy
    properties:
      EndClientUserUniqueSessionId:
        type: string
        default: "Uniquesession"
      EndClientUserBrowserSessionId:
        type: string
        default: ""
      EndClientUserBrowserInformation:
        type: string
        default: ""
      PaaSClientIPAddress:
        type: string
        default: ""
      ClientUniqueRequestID:
        type: string
        default: ""
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "36"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          UserName:
            type: string
            default: "EmbrokerAgent"
          Token:
            type: string
            default: ""
          BrowserIp:
            type: string 
            default: "127.0.0.1"
          LanguageCode:
            type: string
            default: "en"
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
      Policy:
        type: object
