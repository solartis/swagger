swagger: '2.0'
info:
  version: "1.0"
  title: Solartis Cancel API for Embroker
  description: To cancel a policy for Management Liability from Solartis API
  
host: embroker.alpha.solartis.net
basePath: /KnowledgeEngineV6_2/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Cancel the given policy
      operationId: issueCancel
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName  
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string 
          default: "EmbrokerIssueCancel"
        - name: EventVersion
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "1.0.0.1"
        - name: Policy
          in: body
          description: Policy content
          required: true
          schema:
            $ref: '#/definitions/Policy_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Policy_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
            
definitions:
  Policy_Request:
    type: object
    required:
      - ServiceRequestDetail
      - Policy
    properties:
      EndClientUserUniqueSessionId:
        type: string
        default: "Uniquesession"
      EndClientUserBrowserSessionId:
        type: string
      EndClientUserBrowserInformation:
        type: string
      PaaSClientIPAddress:
        type: string
      ClientUniqueRequestID:
        type: string
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "36"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          UserName:
            type: string
            default: "EmbrokerAgent"
          Token:
            type: string
            default: ""
          BrowserIp:
            type: string 
            default: "127.0.0.1"
          LanguageCode:
            type: string
            default: "en"
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
      Policy:
        type: object
        required:
          - PolicyNumber
          - CancelEffectiveDate
          - CancelType
          - EventVersion
          - ReasonForCancellation
        
        properties:
          ProductNumber:
            title: "Product Number"
            type: string
            default: "EMBRKR_ENDR-2"
          ProductVerNumber:
            title: "Product Ver Number"
            type: string
            default: "EMBRKR"
          EventName:
            title: "Event Name"
            type: string
            default: "EmbrokerIssueCancel"
          EventVersion:
            title: "Event Version"
            type: string
            default: "1.0.0.1"
          PolicyNumber:
            title: "Policy Number"
            type: string
          CancelEffectiveDate:
            title: "Cancel Effective Date"
            type: string
          CancelType:
            title: "Cancel Type"
            type: string
            default: "prorata"
          ReasonForCancellation:
            title: "Reason For Cancellation"
            type: string
          OutputTypeList:
            type: array
            items:
              properties:
                Type:
                  title: "Type"
                  type: string
                  enum: ["CANCEL"]
      OwnerId:
        title: "Owner ID"
        type: string
        default: "36"    
        
  Policy_Success_Response:
    type: object
    required:
      - ServiceRequestDetail
      - Policy
    properties:
      EndClientUserUniqueSessionId:
        type: string
        default: "Uniquesession"
      EndClientUserBrowserSessionId:
        type: string
        default: ""
      EndClientUserBrowserInformation:
        type: string
        default: ""
      PaaSClientIPAddress:
        type: string
        default: ""
      ClientUniqueRequestID:
        type: string
        default: ""
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "36"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          UserName:
            type: string
            default: "EmbrokerAgent"
          Token:
            type: string
            default: ""
          BrowserIp:
            type: string 
            default: "127.0.0.1"
          LanguageCode:
            type: string
            default: "en"
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
      Policy:
        type: object
        properties:
          ProductNumber:
            title: "Product Number"
            type: string
          ProductVerNumber:
            title: "Product Ver Number"
            type: string
          EventName:
            title: "Event Name"
            type: string
          EventVersion:
            title: "Event Version"
            type: string
          PolicyNumber:
            title: "Policy Number"
            type: string
          CancelEffectiveDate:
            title: "Cancel Effective Date"
            type: string
          CancelType:
            title: "Cancel Type"
            type: string
          ReasonForCancellation:
            title: "Reason For Cancellation"
            type: string
          OutputTypeList:
            type: array
            items:
              properties:
                Type:
                  title: "Type"
                  type: string
      OwnerId:
        title: "Owner ID"
        type: string
        default: "36"
                
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string      
