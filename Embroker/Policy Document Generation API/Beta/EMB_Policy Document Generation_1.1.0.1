swagger: '2.0'
info:
  version: "1.0"
  title: Solartis Policy Document for Embroker
  description: To generate policy document for Embroker
  
host: embroker.beta.solartis.net
basePath: /KnowledgeEngineV6_2/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Generate Policy Documents for the given input
      operationId: generatepdf
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName  
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "EmbrokerPolicyPDFGeneration"
        - name: EventVersion
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "1.1.0.1"
        - name: Policy
          in: body
          description: Policy content
          required: true
          schema:
            $ref: '#/definitions/Policy_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Policy_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
            
