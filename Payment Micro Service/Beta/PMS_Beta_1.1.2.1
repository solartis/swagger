swagger: '2.0'
info:
  version: 'Beta 1.1.2.1'
  title: Starr Executive Intelligence - Payment Service
  description: >-
    Payment Micro Service is used to process Payment, generate Payment Receipt and email the Receipt to the Payer Email Address.
    
    
    Features:
    1. Payment Processing
    2. Generation of Payment Receipt
    3. Auto Email of Payment Receipt to Payer Email Address
    4. Error Handling (Mandatory, Invalid input validation) included at the Attribute level

host:  starrpmsbeta.solartis.net
basePath: /DroolsV6/DroolsService
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Payment servce is used to process payment, generate receipt and email the receipt 
      operationId: Payment
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: >-
            
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: PaymentMicroService
        - name: EventVersion
          in: header
          description: Name of the event version to be invoked
          required: true
          type: string
          format: string
          default: 1.1.2.1
        - name: Payment
          in: body
          description: Payment/traveler details
          required: true
          schema:
            $ref: '#/definitions/Payment_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Payment_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
            
definitions:
  Payment_Request:
    type: object
    required:
      - ServiceRequestDetail
      - EventName
      - OwnerId
      - PaymentDetail
    properties:
      EndClientUserUniqueSessionId:
        type: string
      EndClientUserBrowserSessionId:
        type: string
      EndClientUserBrowserInformation:
        type: string
      PaaSClientIPAddress:
        type: string
      ClientUniqueRequestID:
        type: string
      ServiceRequestDetail:
        type: object
        required:
          - OwnerId
          - RegionCode
          - Token
          - ServiceRequestVersion
          - ServiceResponseVersion
          - ResponseType
        properties:
          ServiceRequestVersion:
            type: string
            default: '1.0'
          ServiceResponseVersion:
            type: string
            default: '1.0'
          OwnerId:
            type: string
            default: '31'
            enum:
              - '31'
          ResponseType:
            type: string
            enum:
              - JSON
              - XML
            default: JSON
          RegionCode:
            type: string
            default: US
          Token:
            type: string
            default: >-
              
          UserName:
            type: string
            default: pmsunderwriter
          LanguageCode:
            type: string
            default: en
          BrowserIp:
            type: string
      EventName:
        type: string
        default: PaymentMicroService
      OwnerId:
        type: string
        default: '31'
      EventVersion:
        type: string
        default: '1.1.2.1'
      ProductNumber:
        type: string
        default: PMS
      ProductVerNumber:
        type: string
        default: '1.0'
      PaymentDetail:
        type: object
        properties:
          InvoiceNumber:
            type: string
            default: '11221'
          TransactionId:
            description: Transaction ID should be unique for each Transaction
            type: string
            default: '205'
          TransactionType:
            type: string
            default: Report Purchase
            enum:
            - Report Purchase
          Amount:
            type: string
            default: '200.00'
          PaymentMode:
            type: string
            default: Initial
            enum:
            - Initial
          PaymentType:
            type: string
            default: Pay
            enum:
            - Pay
          PaymentMethod:
            type: string
            default: Credit Card
            enum:
            - Credit Card
          TransactionReferenceNumber:
            type: string
            default: '2212'
          Comments:
            type: string
            default: " "
          PaymentMethodDetail:
            type: object
            properties:
              CardDetail:
                type: object
                properties:
                  CardType:
                    type: string
                    enum:
                    - MasterCard
                    - Visa
                    - JCB
                    - Discover
                    - American Express
                  CardNumber:
                    type: string
                    enum:
                    - '5555555555554444'
                    - '4012888888881881'
                    - '3530111333300000'
                    - '6011000990139424'
                    - '347712669659320'
                  ExpiryDate:
                    type: string
                    title: ExpiryDate (MM/YY)
                  CVV:
                    type: string
          PayerDetail:
            type: object
            properties:
              PayerReferenceNumber:
                type: string
                default: '101'
              PayerName:
                type: string
                default: Test Payment
              AddressLine1:
                type: string
                default: '788 County Road'
              AddressLine2:
                type: string
                default: " "
              City:
                type: string
                default: Ider
              State:
                type: string
                default: AL
              CountryCode:
                type: string
                default: US
              ZipCode:
                type: string
                default: '35981'
              PhoneNumber:
                type: string
                default: '1111111111'
              EmailAddress:
                type: string
                default: 'test@test.com'
      PaymentAdditionalDetail:
        type: array
        items:
          properties:
            Key:
              type: string
            Value:
              type: string
          
        
  Payment_Success_Response:
    type: object
    required:
      - ResponseStatus
    properties:
      ResponseStatus:
        type: string
      PaymentDetail:
        type: object
        properties:
          PaymentDate:
            type: string
          PaymentStatus:
            type: string
          PaymentConfirmationNumber:
            type: string
          TransactionReferenceNumber:
            type: string
          PaymentStatusMessage:
            type: string
          PaymentAuthorizationNumber:
            type: string
          PaidAmount:
            type: string
      PayerDetail:
        type: object
        properties:
          PayerReferenceNumber:
            type: string
          PayerName:
            type: string
          EmailAddress:
            type: string
      DocumentDetail:
        type: object
        properties:
          NumberOfDocuments:
            type: string
          Documents:
            type: array
            items:
              properties:
                DocumentType:
                  type: string
                DocumentName:
                  type: string
                DocumentURL:
                  type: string
          
      
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string

