produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Get Policy retrieves specific information about a Policy and a Batch
      operationId: GetPolicy
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: >-
            
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: GetPolicyV2
        - name: GetPolicy
          in: body
          description: GetPolicy/traveler details
          required: true
          schema:
            $ref: '#/definitions/GetPolicy_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/GetPolicy_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
