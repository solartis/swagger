swagger: '2.0'
info:
  version: '1.1.0.1'
  title: EFL Manuscript API
  description: |
    Manuscript API provides ability for underwriters to create “free form” manuscript documents in Microsoft Word, convert the document into .PDF and .XSL, and include them in the Policy Package
    
  contact:
    name: Solartis Support
    email: solartis_assist_support@solartis.com
    
host: documentservice.rc.solartis.net
basePath: /KnowledgeEngineV6_3/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Manuscript API for EFL
      operationId: generateManuscript
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: Content-Type
          in: header
          description: Content Type
          required: true
          type: string
          default: "application/json"
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "ManuscriptFormUpload_1.1.0.1"
        - name: ServiceOwnerID
          in: header
          description: Owner ID of the Service
          required: true
          type: string
          format: string
          default: "503"       
        - name: Manuscript Request
          in: body
          description: For manuscript request
          schema:
            $ref: '#/definitions/ManuscriptGeneration_Request'
            
      responses:
       '200':
        description: Success Response
        schema:
         $ref: '#/definitions/ManuscriptGeneration_Success_Response'
       default:
        description: unexpected error
        schema:
          $ref: '#/definitions/errorModel'
          
definitions:
  ManuscriptGeneration_Request:
    type: object
    properties:
      ServiceRequestDetail:
        type: object
        properties:
         ServiceRequestVersion:
          title: "ServiceRequestVersion"
          type: string
          default: "1.0"
         ServiceResponseVersion:
          title: "ServiceResponseVersion"
          type: string
          default: "1.0"
         OwnerId:
          title: "Owner ID"
          type: string
          default: "503"
         ResponseType:
          title: "Response Type"
          type: string
          default: "json"
         UserName:
          title: "User Name"
          type: string
          default: "StarrGLagent"
         Token:
          type: string
          default: ""
         BrowserIp:
          type: string
          default: "127.0.0.1"
         LanguageCode:
          type: string
          default: "en"
         RegionCode:
          type: string
          default: "US"
      Policy:
       type: object
       properties:
        DocumentInformationList:
          type: array
          items:
            properties:
              DocumentType:
                type: string
              DocumentName:
                type: string
              DocumentEdition:
                type: string
              DocumentNumber:
                type: string
              DocumentFileType:
                type: string
              DocumentByte:
                type: string
              DocumentHierachy:
                type: string
              DocumentSubNumber:
                type: string
        ProductVerNumber:
          type: string
          default: "SOL_SGL_2017_V"
        ProductNumber:
          type: string
          default: "SOL_SGL_V"
        EventName:
          type: string
          default: "ManuscriptFormUpload"
        EventVersion:
          type: string
          default: "1.1.0.1"
        EffectiveDate:
          type: string
        ManuScriptCategory:
          type: string
        ManuScriptCategoryValue:
          type: string
        LOB:
          type: string
          
          
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string
                  
  ManuscriptGeneration_Success_Response:
    type: object
    properties:
      ResponseStatus:
        type: string
      CustomerOwnerID:
        type: string
      Policy:
        type: object
        properties:
          LOB:
            type: string
          ProductVerNumber:
            type: string
          ProductNumber:
            type: string
          EventName:
            type: string
          EventVersion:
            type: string
          EffectiveDate:
            type: string
          ManuScriptCategory:
            type: string
          ManuScriptCategoryValue:
            type: string
          DocumentInformationList:
            type: array
            items:
              properties:
                DocumentType:
                  type: string
                DocumentName:
                  type: string
                DocumentNumber:
                  type: string
                DocumentHierachy:
                  type: string
                DocumentFileType:
                  type: string
                DocumentUID:
                  type: string

        
