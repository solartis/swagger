swagger: '2.0'
info:
  version: '1.0.0.1'
  title: Marine GL - Rating API
  description: |
    Get rates for the Marine GL class types. 
  
    Features:
    1. Accept input as JSON request with Class Type, Deductible, Gross Receipts and Insured  Detail
    2. Respond back with static premium information
    
  contact:
    name: Solartis Support
    email: solartis_assist_support@solartis.com

host: starrassistuatkb.uat.solartis.net
basePath: /DroolsV4_2/DroolsService
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Get Rates for the given input
      operationId: getRates
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "MGLRating"
        - name: EventVersion
          in: header
          description: Name of the version to be invoked
          required: true
          type: string
          format: string
          default: '1.0.0.1'
        - name: Rating
          in: body
          description: Rating content
          required: true
          schema:
            $ref: '#/definitions/Rating_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Rating_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
            
definitions:
  Rating_Request:
    type: object
    required:
     - ServiceRequestDetail
     - QuoteInformation
    properties:
      EndClientUserUniqueSessionId:
        type: string
      EndClientUserBrowserSessionId:
        type: string
      EndClientUserBrowserInformation:
        type: string
      PaaSClientIPAddress:
        type: string
      ClientUniqueRequestID:
        type: string
      ServiceRequestDetail:
        type: object
        required:
          - OwnerId
          - RegionCode
          - Token
          - ServiceRequestVersion
          - ServiceResponseVersion
          - ResponseType
        properties:
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          OwnerId:
            type: string
            default: "34"
          ResponseType:
            type: string
            default: "JSON"
          RegionCode:
            type: string
            default: "US"
          Token:
            type: string
            default: ""
          UserName:
            type: string
            default: "MGLAgent"
          LanguageCode:
            type: string
            default: "en"
          BrowserIp:
            type: string
      QuoteInformation:
        type: object
        required:
          - ProducerCode
          - OwnerId
          - EventName
          - EventVersion
          - ClassType
          - Deductible
          - AnnualGrossReceipts
          - ProductNumber
          - ProductVerNumber
        properties:
          ProductNumber:
            type: string
            default: "MGL"
          ProductVerNumber:
            type: string
            default: "1.0"
          ProducerCode:
            type: string
            default: "5"
          OwnerId:
            type: string
            default: "34"
          ClassType:
            type: string
            enum: ["Small Marine","Artisan Contractor"]
          LimitEachOccurrenceIncludingSupplementaryPayments:
            type: string
            default: "1,000,000 CSL"
          ProductsHazardorCompletedOperations:
            type: string
            default: "2,000,000"
          PersonalInjuryandAdvertisingInjuryLimit:
            type: string
            default: "1,000,000"
          DamagetoPremisesRentedtoyouLimit:
            type: string
            default: "100,000 ANY ONE OCCURRENCE"
          PremisesMedicalPaymentsLimit:
            type: string
            default: "10,000 ANY ONE PERSON"
          GeneralAggregateLimitOtherThanProductsCompletedOperations:
            type: string
            default: "2,000,000"
          PolicyEffectiveDate:
            type: string
          Deductible:
            type: string
            enum: ["1500", "2500"]
          AnnualGrossReceipts:
            type: string
          IsTRIAApplicable:
            type: string
            enum: ["Yes", "No"]
          PrimaryState:
            type: string
          MunicipalityName:
            type: string
          MunicipalityCode:
            type: string
          DoAnyofTheFlowingApply:
            type: string
            enum: ["Yes", "No"]
          DoYouOwnOperateorCharterAnyVessels:
            type: string
            enum: ["Yes", "No"]
          HaveYouHadLiabilityClaimsBroughtAgainstYouInTheLast3Years?:
            type: string
            enum: ["Yes", "No"]
          QuoteType:
            type: string
            default: "New Business"
          EventName:
            type: string
            default: "MGLRating"
          EventVersion:
            type: string
            default: "1.0.0.1"
          Location:
            type: object
            properties:
              LocationDetail:
                type: array
                items:
                  properties:
                    LocationID:
                      type: string
                    AddressLine1:
                      type: string
                    AddressLine2:
                      type: string
                    City:
                      type: string
                    County:
                      type: string
                    State:
                      type: string
                    ZipCode:
                      type: string
                    Classification:
                      type: object
                      properties:
                        ClassificationDetail:
                          type: array
                          items:
                            properties:
                              ClassCode:
                                type: string
                                default: "1629"
                  
  Rating_Success_Response:
    type: object
    required:
      - RequestStatus
      - PremiumInformation
    properties:
      messageDetail:
        type: object
      RequestStatus:
        type: string
      PremiumInformation:
        type: object
        properties:
          ProducerCode:
            type: string
          OwnerId:
            type: string
            default: "34"
          ClassType:
            type: string
            enum: ["Small Marine","Artisan Contractor"]
          LimiteachOccurrenceincludingSupplementaryPayments:
            type: string
            default: "1,000,000 CSL"
          ProductsHazardorCompletedOperations:
            type: string
            default: "2,000,000"
          PersonalInjuryandAdvertisingInjuryLimit:
            type: string
            default: "1,000,000"
          DamagetoPremisesRentedtoyouLimit:
            type: string
            default: "100,000 ANY ONE OCCURRENCE"
          PremisesMedicalPaymentsLimit:
            type: string
            default: "10,000 ANY ONE PERSON"
          GeneralAggregateLimitOtherThanProductsCompletedOperations:
            type: string
            default: "2,000,000"
          PolicyEffectiveDate:
            type: string
          Deductible:
            type: string
            enum: ["1500", "2500"]
          AnnualGrossReceipts:
            type: string
          IsTRIAApplicable:
            type: string
            enum: ["Yes", "No"]
          PrimaryState:
            type: string
          MunicipalityName:
            type: string
          MunicipalityCode:
            type: string
          DoAnyofTheFlowingApply:
            type: string
            enum: ["Yes", "No"]
          DoYouOwnOperateorCharterAnyVessels:
            type: string
            enum: ["Yes", "No"]
          HaveYouHadLiabilityClaimsBroughtAgainstYouInTheLast3Years?:
            type: string
            enum: ["Yes", "No"]
          QuoteType:
            type: string
            default: "New Business"
          EventName:
            type: string
            default: "MGLRating"
          EventVersion:
            type: string
            default: "1.0.0.1"
          TFSList:
            type: array
            items:
              properties:
                TFSDetails:
                  type: object
                  properties:
                    TFSName:
                      type: string
                    TFSRate:
                      type: string
                    TFSAmount:
                      type: string
          TotalBasePremium:
            type: string
          TotalTFSPremium:
            type: string
          TRIAPremium:
            type: string
          TotalPolicyPremium:
            type: string
  
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string
