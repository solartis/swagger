swagger: '2.0'
info:
  version: 'Alpha 1.1.1.1'
  title: Marine GL - Cancel API
  description: |
    To cancel Marine GL policies. 
  
    Features:
    1. Accepts Policy Number, Cancel Type, Cancel Reason and Cancel Effective Date as input. Based on the cancel type premium will be returned.
    2. Error handling included at each attribute level.
    
  contact:
    name: Solartis Support
    email: solartis_assist_support@solartis.com
  
host: starrmarinegl.beta.solartis.net
basePath: /KnowledgeEngineV6_2/KnowledgeBase
schemes:
  - https
consumes:
  - application/json  
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Cancel rate API for issued Marine GL policies.
      operationId: Cancelrate
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        
        - name: Content-Type
          in: header
          description: Content Type
          required: true
          type: string
          default: "application/json"
          
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "MGLCancel"
          
        - name: EventVersion
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "1.1.1.1"
        
        - name: Cancel_request
          in: body
          description: For cancel_request
          schema:
            $ref: '#/definitions/cancel_request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Cancel_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
definitions:
  cancel_request:
    type: object
    properties:
       EndClientUserUniqueSessionId:
        type: string
       ServiceRequestDetail:
        type: object
        properties:
          ServiceRequestVersion:
           type: string
           default: "1.0"
          ServiceResponseVersion:
           type: string
           default: "1.0"
          OwnerId:
            type: string
            default: "34"
            enum: ["34"]
          ResponseType:
           type: string
           default: "json"
          UserName:
           type: string
           default: "MGLAgent"
          Token:
           type: string
           default: ""
          BrowserIp:
            type: string
          LanguageCode:
            type: string
            default: "en"    
          RegionCode:
            type: string
            default: "US"  
       PolicyInformation:
         type: object
         required:
          - ProductNumber
          - ProductVerNumber
          - EventName
          - EventVersion
          - PolicyNumber
          - CancelEffectiveDate
          - CancelReason
          - CancelType
         properties:
           ProductNumber:
            type: string
            default: "MGL_SM"
            enum: ["MGL_SM", "MGL_MA"]
           ProductVerNumber:
            type: string
            default: "1.0"
           OwnerId:
            type: string
            default: "34"
            enum: ["34"]
           EventName:
            type: string
            default: "MGLCancel"
           EventVersion: 
            type: string
            default: "1.1.1.1"
            enum: ["1.1.1.1"]
           PolicyNumber:
            type: string
           CancelEffectiveDate:
            title: Cancel Effective Date (MM/DD/YYYY)
            type: string
           CancelReason:
            type: string
            enum: ["Insured's Option - No insurable interest","Insured's Option - Other","Company Option","Cancel - Policy Change","Non-Payment of Premium"]
            default: "Insured's Option - No insurable interest"
           CancelType:
            type: string
            enum: ["Flat", "Prorata","Short rate"]
            default: "Flat"
           
  Cancel_Success_Response:
    type: object
    required:
      - RequestStatus
      - PolicyInformation
    properties:
      RequestStatus:
        type: string
      PolicyInformation:
       type: object
       properties:
        ProductNumber:
         type: string
        ProductVerNumber: 
         type: string
        EventName: 
         type: string
        EventVersion:
         type: string
        PolicyNumber:
         type: string
        CancelType:
         type: string
        CancelEffectiveDate:
         type: string
        CancelReason:
         type: string
        TotalReturnpremium:
         type: string
        Documents:
          type: array
          items:
            properties:
              DocumentType:
                type: string
              DocumentURL:
                type: string
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
        default: "FAILED"
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string
        
