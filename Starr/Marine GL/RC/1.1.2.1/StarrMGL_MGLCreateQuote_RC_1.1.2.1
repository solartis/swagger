swagger: '2.0'
info:
  version: 'RC 1.1.2.1'
  title: Marine GL - Quote API
  description: |
    Get Quote for the Marine GL class types. 
  
    Features:
    1. Accept input as JSON request with Class Type, Deductible, Gross Receipts, Insured and location details
    2. Provided information will be saved and Quote Number will be generated.
    3. Error handling included at each attribute level
    
  contact:
    name: Solartis Support
    email: solartis_assist_support@solartis.com

host: starrmarinegl.rc.solartis.net
basePath: /KnowledgeEngineV6_2/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: Create Quote for the given input
      operationId: createQuote
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "MGLCreateQuote"
        - name: EventVersion
          in: header
          description: Name of the version to be invoked
          required: true
          type: string
          format: string
          default: '1.1.2.1'
        - name: Quote
          in: body
          description: Quoting content
          required: true
          schema:
            $ref: '#/definitions/Quote_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Quote_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
definitions:
  Quote_Request:
    type: object
    required:
     - ServiceRequestDetail
     - QuoteInformation
    properties:
      EndClientUserUniqueSessionId:
        type: string
      EndClientUserBrowserSessionId:
        type: string
      EndClientUserBrowserInformation:
        type: string
      PaaSClientIPAddress:
        type: string
      ClientUniqueRequestID:
        type: string
      ServiceRequestDetail:
        type: object
        required:
          - OwnerId
          - RegionCode
          - Token
          - ServiceRequestVersion
          - ServiceResponseVersion
          - ResponseType
        properties:
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          OwnerId:
            type: string
            default: "34"
          ResponseType:
            type: string
            default: "JSON"
          RegionCode:
            type: string
            default: "US"
          Token:
            type: string
            default: ""
          UserName:
            type: string
            default: "mglagent"
          LanguageCode:
            type: string
            default: "en"
          BrowserIp:
            type: string
      QuoteInformation:
        type: object
        required:
          - OwnerId
          - StateCode
          - EventName
          - EventVersion
          - ClassType
          - Deductible
          - AnnualGrossReceipts
          - ProductNumber
          - ProductVerNumber
          - Location
        properties:
          ProductNumber:
            type: string
            enum: ["MGL_SM", "MGL_MA"]
          ProductVerNumber:
            type: string
            default: "1.0"
          OwnerId:
            type: string
            default: "34"
          ProductionChannel:
            type: string
            enum: ["MGL_DTC", "MGL_BTIS"]
          ClassType:
            type: string
            enum: ["Small Marine","Marine Artisan"]
          LimitEachOccurrenceIncludingSupplementaryPayments:
            type: string
            default: "1,000,000 CSL"
          ProductsHazardorCompletedOperations:
            type: string
            default: "2,000,000"
          PersonalInjuryandAdvertisingInjuryLimit:
            type: string
            default: "1,000,000"
          DamagetoPremisesRentedtoyouLimit:
            type: string
            default: "100,000 ANY ONE OCCURRENCE"
          PremisesMedicalPaymentsLimit:
            type: string
            default: "10,000 ANY ONE PERSON"
          GeneralAggregateLimitOtherThanProductsCompletedOperations:
            type: string
            default: "2,000,000"
          PolicyEffectiveDate:
            type: string
            title: Effective Date (MM/DD/YYYY)
          Deductible:
            type: string
            enum: ["1500", "2500", "5000", "10000"]
          AnnualGrossReceipts:
            type: string
          LiabilityLimit:
            type: string
            enum: ['1000000', '500000', '300000']
          IsTRIAApplicable:
            type: string
            enum: ["Yes", "No"]
          BusinessName:
            type: string
          MailingAddressLine1:
            type: string
          MailingAddressLine2:
            type: string
          MailingCity:
            type: string
          MailingCountry:
            type: string
            default: United States
          MailingStateCode:
            type: string
          MailingState:
            type: string
          MailingZipCode:
            type: string
          InsuredFirstName:
            type: string
          InsuredLastName:
            type: string
          AddressLine1:
            type: string
          AddressLine2:
            type: string
          City:
            type: string
          Country:
            type: string
            default: United States
          StateCode:
            type: string
          State:
            type: string
          ZipCode:
            type: string
          PhoneNumber:
            type: string
          EmailAddress:
            type: string
          MunicipalityName:
            type: string
          MunicipalityCode:
            type: string
          DoAnyofTheFollowingApply:
            type: string
            enum: ["Yes", "No"]
          DoYouOwnOperateorCharterAnyVessels:
            type: string
            enum: ["Yes", "No"]
          HaveYouHadLiabilityClaimsBroughtAgainstYouInTheLast3Years?:
            type: string
            enum: ["Yes", "No"]
          OrganizationType:
            type: string
          QuoteType:
            type: string
            default: "New Business"
          EventName:
            type: string
            default: "MGLCreateQuote"
          EventVersion:
            type: string
            default: "1.1.2.1"
          DescriptionOfOperations:
                      type: object
                      properties:
                        DescriptionOfOperationsList:
                          type: array
                          items:
                            properties:
                              Operations:
                                type: string
          Location:
            type: object
            properties:
              LocationDetail:
                type: array
                items:
                  properties:
                    LocationID:
                      type: string
                    AddressLine1:
                      type: string
                    AddressLine2:
                      type: string
                    City:
                      type: string
                    Country:
                      type: string
                      default: United States
                    State:
                      type: string
                      description: Please provide State Name
                    ZipCode:
                      type: string
                    Classification:
                      type: object
                      properties:
                        ClassificationDetail:
                          type: array
                          items:
                            properties:
                              ClassCode:
                                type: string
                                default: "1629"
                  
