swagger: '2.0'
info:
  version: '1.1.0.1'
  title: Starr Assist - Pay Issue API
  description: |
    This version of API is used to get Trip, Traveler, Payer and Payment information from the Customer and process Payment with the provided information, issue Policies and generates respective documents.

      An Auto email will be sent to the Payer Email address with the generated documents.
  
    Features:
    1. Provides ability to process payment for the information provided and issues Policy for each Traveler in the Batch
    
  contact:
    name: Solartis Support
    email: solartis_assist_support@solartis.com

host: qasakbv4-2.solartis.net
basePath: /DroolsV4_2/DroolsService
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV2:
    post:
      description: This API issues the Policy with the information provided
      operationId: Pay_Issue
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: >-
            
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: 'SQM_PayIssue_1.1.0.1'
        - name: Pay_Issue
          in: body
          description: Pay_Issue/traveler details
          required: true
          schema:
            $ref: '#/definitions/Pay_Issue_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/Pay_Issue_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'
definitions:
  Pay_Issue_Request:
    type: object
    required:
      - ServiceRequestDetail
      - PolicyInformation
    properties:
      EndClientUserUniqueSessionId:
        type: string
      EndClientUserBrowserSessionId:
        type: string
      EndClientUserBrowserInformation:
        type: string
      PaaSClientIPAddress:
        type: string
      ClientUniqueRequestID:
        type: string
      ServiceRequestDetail:
        type: object
        required:
          - OwnerId
          - RegionCode
          - Token
          - ServiceRequestVersion
          - ServiceResponseVersion
          - ResponseType
        properties:
          ServiceRequestVersion:
            type: string
            default: '1.0'
          ServiceResponseVersion:
            type: string
            default: '1.0'
          OwnerId:
            type: string
            default: '15'
            enum:
              - '15'
          ResponseType:
            type: string
            enum:
              - JSON
              - XML
            default: JSON
          RegionCode:
            type: string
            default: US
          Token:
            type: string
            default: >-
              
          UserName:
            type: string
            default: starragent
          LanguageCode:
            type: string
            default: en
          BrowserIp:
            type: string
      PolicyInformation:
        type: object
        required:
          - ProducerCode
          - ProductVerID
          - ProductID
          - ProductNumber
          - ProductVerNumber
          - OwnerId
        properties:
          ProductVerID:
            type: string
            default: '706'
          ProductID:
            type: string
            default: '619'
          ProductNumber:
            type: string
            default: ILT
          ProductVerNumber:
            type: string
            default: '1.0'
          ProducerCode:
            type: string
            default: '8946'
          OwnerId:
            type: string
            default: '15'
            enum:
              - '15'
          RoleID:
            type: string
            default: '5'
          RoleName:
            type: string
            default: Agent
          RoleType:
            type: string
            default: User
          EventName:
            type: string
            default: 'SQM_PayIssue_1.1.0.1'
            enum:
              - 'SQM_PayIssue_1.1.0.1'
          PlanName:
            type: string
            default: 'CAT 70'
            enum: ['CAT 70']
          PlanCode:
            type: string
            default: '15'
            enum: ['15']
          StateCode:
            type: string
          StateName:
            type: string
          DepartureDate:
            type: string
            title: Departure Date(MM/DD/YYYY)
          ReturnDate:
            type: string
            title: Return Date(MM/DD/YYYY)
          DepositDate:
            type: string
            title: Deposit Date(MM/DD/YYYY)
          Premium:
            type: string
          TripCancellationCoverage:
            type: string
            enum: ["With Trip Cancellation","With Trip Cancellation for Any Reason"]
            default: With Trip Cancellation
          DestinationCountry:
            type: string
          CubaTravelReason:
            type: string
            enum: ["Family visits", "Official business of the U.S. government, foreign governments, and certain intergovernmental organizations", "Journalistic activity", "Professional research and professional meetings", "Educational activities", "Religious activities", "Public performances, clinics, workshops, athletic and other competitions, and exhibitions", "Support for the Cuban people", "Humanitarian projects", "Activities of private foundations or research or educational institutes", "Exportation, importation, or transmission of information or information materials", "Certain authorized export transactions", "Other Licensed Activity"]
          TravelerList:
            type: array
            items:
              properties:
                TravelerDOB:
                  type: string
                TravelCost:
                  type: string
                FirstName:
                  type: string
                LastName:
                  type: string
                AddressLine1:
                  type: string
                AddressLine2:
                  type: string
                City:
                  type: string
                State:
                  type: string
                StateCode:
                  type: string
                Country:
                  type: string
                  default: "United States"
                Zipcode:
                  type: string
                Phone:
                  type: string
                Email:
                  type: string
                OFACLicenseNumber:
                  type: string
                TravelerIndex:
                  type: string
          PaymentInformation:
            type: object
            properties:
                CardNumber:
                  type: string
                  enum:
                    - '5555555555554444'
                    - '4012888888881881'
                    - '3530111333300000'
                    - '6011000990139424'
                    - '347712669659320'
                CVV:
                  type: string
                  default: '123'
                ExpiryMonth:
                  type: string
                ExpiryYear:
                  type: string
                PayerName:
                  type: string
                PayerAddress1:
                  type: string
                PayerAddress2:
                  type: string
                PayerCity:
                  type: string
                PayerState:
                  type: string
                  description: Provide Payer State Code
                PayerCountry:
                  type: string
                PayerZipcode:
                  type: string
                PayerEmail:
                  type: string
                PayerPhone:
                  type: string
                PaymentMethod:
                  type: string
                  enum:
                    - Credit Card
                CardType:
                  type: string
                  enum:
                    - MasterCard
                    - Visa
                    - JCB
                    - Discover
                    - American Express
                    
  Pay_Issue_Success_Response:
    type: object
    required:
      - RequestStatus
      - PolicyBatch
    properties:
      messageDetail:
        type: object
      RequestStatus:
        type: string
      CustomerName:
        type: string
      PolicyBatch:
        type: object
        properties:
          PaymentStatus:
            type: string
          DepartureDate:
            type: string
          ReturnDate:
            type: string
          PolicyEffectiveDate:
            type: string
          PolicyExpirationDate:
            type: string
          DepositDate:
            type: string
          BatchNumber:
            type: string
          CalculatedPolicyPremium:
            type: string
          Documents:
            type: array
            items:
              properties:
                DocumentType:
                  type: string
                DocumentName:
                  type: string
                DocumentURL:
                  type: string
          Policies:
            type: array
            items:
              properties:
                PolicyNumber:
                  type: string
                TravelerName:
                  type: string
                TravelerIndex:
                  type: string
                TravelerPremium:
                  type: string
                Documents:
                  type: array
                  items:
                    properties:
                      DocumentType:
                        type: string
                      DocumentName:
                        type: string
                      DocumentURL:
                        type: string
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string
